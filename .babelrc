{
  "presets": [
    [
    "@babel/preset-env",
    {
      "modules": false
    }
  ],
    ["@babel/preset-react", {"throwIfNamespace": false}]
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": {
          "version": 3,
          "proposals": true
        },
        "useESModules": true
      }
    ],
    "react-html-attrs"
  ]
}
